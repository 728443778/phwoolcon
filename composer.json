{
    "name": "phwoolcon/phwoolcon",
    "description": "Phalcon + Swoole",
    "keywords": ["phwoolcon", "phalcon", "swoole"],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Christopher CHEN",
            "email": "fishdrowned@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mcrypt": "*",
        "ext-openssl": "*",
        "ext-PDO": "*",
        "phwoolcon/fsm": "~1.0",
        "symfony/console": "3.0.*",
        "pda/pheanstalk": "3.1.0",
        "swiftmailer/swiftmailer": "5.4.2"
    },
    "require-dev": {
        "techpivot/phalcon-ci-installer": "~1.0"
    },
    "suggest": {
        "ext-phalcon": ">=3.0.0",
        "ext-swoole": "*"
    },
    "autoload": {
        "psr-4": {
            "Phwoolcon\\": "src",
            "Phwoolcon\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "/tests/"
        ],
        "files": ["functions.php"]
    },
    "bin": [
        "phwoolcon-package/phwoolcon-package-phwoolcon.php"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
